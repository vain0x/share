{
  "name": "share",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/vain0x/share",
  "author": "vain0x",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "yarn run prod",
    "check": "tsc --noEmit --watch",
    "dev": "yarn && run-p dev:*",
    "dev:server-build": "tsc -w",
    "dev:server-serve": "cross-env NODE_ENV=development WEB_PORT=8080 WEB_HOME=/ node scripts/serve.js",
    "prod": "yarn && run-s prod:check prod:server-serve",
    "prod:check": "tsc --noEmit",
    "prod:server-serve": "cross-env NODE_ENV=production ts-node ./src/server/index.ts"
  },
  "dependencies": {
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/node": "^12.6.9",
    "chokidar": "^3.0.2",
    "cross-env": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
